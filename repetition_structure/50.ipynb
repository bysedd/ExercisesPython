   "source": [
    "# Sendo H= 1 + 1/2 + 1/3 + 1/4 + ... + 1/N, Fa√ßa um programa que calcule o valor de H com N termos."
   ],
   "metadata": {
    "collapsed": false
   }
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "outputs": [],
   "source": [
    "n = int(input('Digite um inteiro positivo:'))\n",
    "h = 2\n",
    "calc = [1]"
   ],
   "metadata": {
    "collapsed": false
   }
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "outputs": [],
   "source": [
    "for i in range(1, n + 1):\n",
    "    calc.append(i/h)\n",
    "    h += 1"
   ],
   "metadata": {
    "collapsed": false
   }
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "4.55\n"
     ]
    }
   ],
   "source": [
    "print(round(sum(calc), 5))"
   ],
