   "outputs": [],
   "source": [
    "notas = [float(input(f'Digite a {n + 1}º nota')) for n in range(4)]"
   ],
   "metadata": {
    "collapsed": false
   }
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Notas: [5.0, 2.0, 10.0, 20.0]\n",
      "Média: 9.2\n"
     ]
    }
   ],
   "source": [
    "print(f'Notas: {notas}')\n",
    "print(f'Média: {round(sum(notas) / len(notas), 1)}')"
   ],
